{
  "source":"deterministic",
  "name":"Deterministic",
  "module":"deterministic",
  "quartz":{
    "init" : [
      "func init"
    ],
    "assets" : [
      "func assets"
    ],
    "code/baseMode" : [
      "mime file:data",
      "done 'modules/deterministic/$baseMode/deterministic.js.lzma'"
    ],
    "hash/modeOrSymbol" : [
      "func hash/$modeOrSymbol"
    ],
    "hashes" : [
      "func hashes"
    ],
    "modes" : [
      "func modes"
    ],
    "reload/baseMode" : [
      "func reload/$baseMode"
    ]
  },
  "router" : {
    "assets":"List available assets.",
    "code": {
      "_ref":{
        "_list" : "mode",
        "_sample" : "dummycoin",
        "_this" : "Returns the lzma compressed deterministic blobs (client side code) for a `$MODE` ."
      }
    },
    "hash":  {
      "_ref":{
        "_list" : "mode",
        "_sample" : "dummycoin",
        "_this" : "Returns the DJB2 hash of the deterministic blobs (client side code) for `$MODE` ."
      }
    },
    "reload" : {
      "_access" : "root",
      "_this" : "Reload all deterministic blobs",
      "_ref":{
        "_list" : "mode",
        "_sample" : "dummycoin",
        "_this" : "Reload the deterministic blob (client side code) for `$MODE` ."
      }
    },
    "hashes":"Returns the DJB2 hash of all deterministic blobs (client side code).",
    "modes":"List available modes."
  }
}
