{
  "symbol":"eth.hy",
  "name":"hybrix multi-chain asset (ETH)",
  "module":"ethereum",
  "import":"eth",
  "mode":"ethereum.token",
  "contract":"0x9b53e429b0badd98ef7f01f03702986c516a5715",
  "originator":"0x6bCBa1899E6F25a2369C99E5caC6943A9cFab287",
  "factor":18,
  "feeContractTx":0.0025,
  "notes":{
    "send":"Note that HY on Ethereum has an expensive ETH transaction fee!"
  },
  "quartz":{
    "unspent/source/amount/target/publicKey" : [
      "with nativeBalance rout /a/eth/balance/$source",
      "with tokenFee [call,fee] [ship,2,1,1,1] [done,$feeContractTx] [math,'*2']",
      "true '$nativeBalance > $tokenFee' 2 1",
      "fail 'Not enough ETH balance on account to pay transaction fee! Balance needed is $tokenFee ETH, while available only $nativeBalance ETH.'",
      "call unspentData/$source/$amount/$target/$publicKey"
    ]
  }
}
