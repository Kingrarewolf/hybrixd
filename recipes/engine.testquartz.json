{
  "engine": "testquartz",
  "name": "Quartz Testing Engine",
  "OK": "OK",
  "NOT_OK": "NOT_OK",
  "quartz": {
    "test": [
      "# NOTE: ALL TESTS ARE DEFINED IN THE lib/scheduler/methods METHOD files",
      "qrtz",
      "excl [test,subtest] true",
      "each subtest",
      "excl $OK",
      "poke testResults",
      "size",
      "flow 0 1 2",
      "done $OK",
      "fail $testResults"
    ],
    "subtest": [
      "poke method ${.value}",
      "hook @fail",
      "call '${.value}'",
      "done",
      "@fail",
      "done '$method failed : $'"
    ]
  },
  "router": {
    "_hidden": true,
    "_ellipsis": true
  }
}
